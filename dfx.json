{
  "version": 1,
  "canisters": {
    "custody_wallet": {
      "type": "custom",
      "package": "custody_wallet",
      "candid": "src/custody_wallet/custody_wallet.did",
      "wasm": "target/wasm32-unknown-unknown/release/custody_wallet.wasm",
      "build": "src/custody_wallet/build.sh",
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    },
    "fiduciary": {
      "main": "src/fiduciary/Main.mo",
        "type": "motoko",
        "declarations": {
          "output": "src/fiduciary/declarations/"
        }
    }
  },
  "defaults": {
    "bitcoin": {
      "enabled": true,
      "nodes": [
        "127.0.0.1:18444"
      ],
      "log_level": "info"
    },
    "build": {
      "packtool": "",
      "args": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943"
    }
  }
}
